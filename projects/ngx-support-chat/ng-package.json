{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ngx-support-chat",
  "lib": {
    "entryFile": "src/public-api.ts",
    "cssUrl": "inline"
  },
  "assets": [
    "schematics/**/*.json",
    {
      "input": "src/styles",
      "glob": "**/*.css",
      "output": "styles"
    }
  ]
}
